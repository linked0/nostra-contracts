{
  "name": "@nostra/contracts",
  "version": "1.0.0",
  "description": "Nostra Prediction Market Smart Contracts",
  "main": "index.js",
  "private": true,
  "scripts": {
    "compile": "hardhat compile",
    "test": "hardhat test",
    "test:unit": "hardhat test test/unit/*.test.ts",
    "test:integration": "hardhat test test/integration/*.test.ts",
    "coverage": "hardhat coverage",
    "deploy:local": "hardhat run scripts/deploy/deploy-local.ts --network localhost",
