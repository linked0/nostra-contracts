    "deploy:polygon-amoy": "hardhat run scripts/deploy/deploy.ts --network polygonAmoy",
    "deploy:bsc": "hardhat run scripts/deploy/deploy-bsc.ts --network bsc",
    "deploy:bsc-testnet": "hardhat run scripts/deploy/deploy-bsc-testnet.ts --network bscTestnet",
    "verify": "hardhat verify",
    "clean": "hardhat clean",
    "size": "hardhat size-contracts"
